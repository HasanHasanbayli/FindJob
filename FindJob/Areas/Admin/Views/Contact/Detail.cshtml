@model ContactFromUser
@{
    ViewData["Title"] = "Detail";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

<div class="col-12">
    <div class="form-group mt-3">
        <label asp-for="FullName"></label>
        <input asp-for="FullName" class="form-control" placeholder="Fullname" disabled>
    </div>
    <div class="form-group mt-3">
        <label asp-for="Email"></label>
        <input asp-for="Email" class="form-control" placeholder="Email" disabled>
    </div>
    <div class="form-group">
        <label asp-for="PhoneNumber"></label>
        <input asp-for="PhoneNumber" class="form-control" placeholder="Subject" disabled/>
    </div>
    <div class="form-group">
        <label asp-for="Subject"></label>
        <input asp-for="Subject" class="form-control" placeholder="Subject" disabled/>
    </div>
    <div class="form-group">
        <label asp-for="Message"></label>
        <textarea asp-for="Message" class="form-control" placeholder="Message" disabled></textarea>
    </div>
</div>
<div class="col-12 mt-3">
    <a asp-action="Index" class="btn btn-dark">Go Back</a>
</div>